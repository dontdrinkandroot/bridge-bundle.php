{
    "name": "dontdrinkandroot/bridge-bundle",
    "type": "symfony-bundle",
    "description": "Dontdrinkandroot specific bridges",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\BridgeBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": "^8.1",
        "dontdrinkandroot/common": "dev-master",
        "symfony/config": "^4.4|^5.4",
        "symfony/dependency-injection": "^4.4|^5.4",
        "symfony/http-kernel": "^4.4|^5.4",
        "symfony/yaml": "^4.4|^5.4"
    },
    "require-dev": {
        "dontdrinkandroot/api-platform-bundle": "dev-master",
        "dontdrinkandroot/bootstrap-bundle": "dev-master",
        "dontdrinkandroot/crud-admin-bundle": "dev-master",
        "dontdrinkandroot/doctrine-bundle": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "liip/test-fixtures-bundle": "^1.0",
        "phpunit/phpunit": "^9.0",
        "symfony/phpunit-bridge": "^4.4"
    },
    "config": {
        "sort-packages": true
    }
}
